tabs:
  - test.ipynb

terminals:
  - |
    pip install notebook --user
    export PATH="$HOME/.local/bin:$PATH"
    jupyter notebook test.ipynb --no-browser --port=1337 --ip=0.0.0.0 --NotebookApp.token='' --NotebookApp.password=''

run-button: |
  if [[ "$$file" == *.py ]]; then
    python3 "$$file"
  else
    echo "‚ùå Run button only works for .py files. Open test.ipynb at https://whole-swing.run-code.com to run the notebook."
  fi

browser-link: https://whole-swing.run-code.com
